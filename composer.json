{
    "name": "datadome/zendsearch",
    "description": "a general purpose text search engine written entirely in PHP 5",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://packages.zendframework.com/",
    "autoload": {
        "psr-0": {
            "ZendSearch": "library/",
            "ZendSearchTest": "tests/ZendSearch/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laminas/laminas-stdlib": "3.*",
        "ext-iconv": "*",
        "symfony/polyfill-php80": "^1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
